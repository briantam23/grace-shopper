{
  "name": "grace-shopper",
  "version": "1.0.0",
  "description": "A Single Page App created with React, Redux, JWT Authentication, and the Stripe API.",
  "main": "server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "heroku-postbuild": "webpack -p",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "build": "npm run webpack -- --watch --mode=development",
    "start:dev": "npm run build & DATABASE_URL=postgres://localhost/grace-shopper & JWT_SECRET='foo' nodemon server/index.js --ignore public --ignore src",
    "test": "mocha ./test/*.test.js --watch --require babel-polyfill --compilers js:babel-register"
  },
  "keywords": ["react", "redux", "jwt-authentication", "stripe-api"],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "jwt-simple": "^0.5.5",
    "pg": "^7.4.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-loading": "^2.0.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.0.1",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^5.0.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.15.1",
    "stripe": "^7.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^3.2.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  }
}
